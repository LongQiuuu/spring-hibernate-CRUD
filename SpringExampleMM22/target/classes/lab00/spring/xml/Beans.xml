<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	<!-- 組態資訊撰寫 -->
	<!-- <bean id='lottery' class='物件所許類別' > -->
	<bean id='lottery' class='lab00.spring.xml.LotteryService' >
	<!-- 下列依賴注入 -->
	   <property name="lowerBound" value='1'></property>
	   <property name="upperBound" value='10'></property>
	   <property name="ballNumber" value='3'></property>
	</bean>
	
	<bean id='lottery2' class='lab00.spring.xml.LotteryService' >
	<!-- 下列依賴注入 -->
	   <property name="lowerBound" value='1'></property>
	   <property name="upperBound" value='10'></property>
	   <property name="ballNumber" value='3'></property>
	</bean>

	
	
</beans>